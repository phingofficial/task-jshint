{
  "name": "phing/task-jshint",
  "description": "Checks the JavaScript code using JSHint.",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "LGPL-3.0-only",
  "type": "phing-extension",
  "authors": [
    {
      "name": "Siad Ardroumli",
      "email": "siad.ardroumli@gmail.com"
    }
  ],
  "require": {
    "php": ">= 7.4",
    "ext-libxml": "*",
    "ext-simplexml": "*"
  },
  "require-dev": {
    "phing/phing": "3.0.x-dev"
  },
  "autoload": {
    "psr-4": {
      "Phing\\Task\\Ext\\JsHint\\": ""
    }
  },
  "extra": {
    "phing-custom-taskdefs": {
      "jshint": "Phing\\Task\\Ext\\JsHint\\JsHintTask"
    }
  }
}
